{
  "name": "quiz-app-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "cp .env.staging .env.local 2>/dev/null || true && next dev",
    "dev:production": "cp .env.production .env.local 2>/dev/null || true && next dev",
    "build": "next build",
    "build:staging": "NEXT_PUBLIC_APP_ENV=staging next build",
    "build:production": "NEXT_PUBLIC_APP_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:restart": "npx supabase stop && npx supabase start",
    "supabase:status": "npx supabase status",
    "supabase:studio": "npx supabase studio",
    "db:migrate": "npx supabase db reset",
    "db:seed": "npx supabase seed",
    "db:reset": "npx supabase db reset",
    "test": "echo 'No tests yet' && exit 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "postcss": "^8.5.6",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  }
}
